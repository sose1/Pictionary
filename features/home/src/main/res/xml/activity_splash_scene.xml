<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:autoTransition="animateToEnd"
        motion:duration="5000">
       <KeyFrameSet>
           <KeyAttribute
               motion:motionTarget="@+id/k"
               motion:framePosition="1"
               android:alpha="0.0"
               android:translationY="-600dp" />
           <KeyAttribute
               motion:motionTarget="@+id/ludek"
               motion:framePosition="1"
               android:alpha="0.0"
               android:translationY="600dp" />


           <KeyAttribute
               motion:motionTarget="@+id/k"
               motion:framePosition="6"
               android:alpha="0.0"
               android:translationY="-600dp" />
           <KeyAttribute
               motion:motionTarget="@+id/ludek"
               motion:framePosition="6"
               android:alpha="0.0"
               android:translationY="600dp" />


           <KeyAttribute
               motion:motionTarget="@+id/k"
               motion:framePosition="12"
               android:alpha="1.0"
               android:translationY="3dp" />
           <KeyAttribute
               motion:motionTarget="@+id/ludek"
               motion:framePosition="12"
               android:alpha="1.0"
               android:translationY="3dp" />
           <KeyAttribute
               motion:motionTarget="@+id/k"
               motion:framePosition="13"
               android:alpha="1.0"
               android:translationY="4dp" />
           <KeyAttribute
               motion:motionTarget="@+id/ludek"
               motion:framePosition="13"
               android:alpha="1.0"
               android:translationY="4dp" />


           <KeyAttribute
               motion:motionTarget="@+id/ludek"
               motion:framePosition="20"
               android:alpha="1.0"
               android:translationY="60dp" />
           <KeyAttribute
               motion:motionTarget="@+id/k"
               motion:framePosition="20"
               android:alpha="1.0"
               android:translationY="60dp" />


           <KeyAttribute
               motion:motionTarget="@+id/k"
               motion:framePosition="27"
               android:alpha="1.0"
               android:translationY="0dp" />
           <KeyAttribute
               motion:motionTarget="@+id/ludek"
               motion:framePosition="27"
               android:alpha="1.0"
               android:translationY="4dp" />


           <KeyAttribute
               motion:motionTarget="@+id/k"
               motion:framePosition="34"
               android:alpha="1.0"
               android:scaleY="1.0"
               android:scaleX="1.0"
               android:translationY="0dp" />
           <KeyAttribute
               motion:motionTarget="@+id/ludek"
               motion:framePosition="34"
               android:alpha="1.0"
               android:scaleY="1.0"
               android:scaleX="1.0"
               android:translationY="4dp" />
           <KeyAttribute
               motion:motionTarget="@+id/k"
               motion:framePosition="50"
               android:alpha="1.0"
               android:scaleY="1.3"
               android:scaleX="1.3"
               android:translationY="0dp" />
           <KeyAttribute
               motion:motionTarget="@+id/ludek"
               motion:framePosition="50"
               android:alpha="1.0"
               android:scaleY="1.3"
               android:scaleX="1.3"
               android:translationY="4dp" />



           <KeyAttribute
               motion:motionTarget="@+id/k"
               motion:framePosition="64"
               android:alpha="1.0"
               android:scaleY="0.3"
               android:scaleX="0.3"
               android:translationY="300dp" />
           <KeyAttribute
               motion:motionTarget="@+id/ludek"
               motion:framePosition="64"
               android:alpha="1.0"
               android:scaleY="0.3"
               android:scaleX="0.3"
               android:translationY="300dp" />


           <KeyAttribute
               motion:motionTarget="@+id/blue_screen"
               motion:framePosition="39"
               android:alpha="0.0"
               android:translationY="-1000dp" />
           <KeyAttribute
               motion:motionTarget="@+id/blue_screen"
               motion:framePosition="40"
               android:alpha="1.0"
               android:translationY="-1000dp" />
           <KeyAttribute
               motion:motionTarget="@+id/blue_screen"
               motion:framePosition="65"
               android:translationY="0dp"
               android:alpha="1.0" />


           <KeyAttribute
               motion:motionTarget="@+id/pictionary"
               motion:framePosition="41"
               android:alpha="0.0"
               android:translationY="-600dp"
               android:scaleX="-0.5"
               android:scaleY="-0.5" />
           <KeyAttribute
               motion:motionTarget="@+id/pictionary"
               motion:framePosition="55"
               android:alpha="0.5"
               android:translationY="-200dp"
               android:scaleX="0.5"
               android:scaleY="0.5" />


           <KeyAttribute
               motion:motionTarget="@+id/pictionary"
               motion:framePosition="70"
               android:alpha="1.0"
               android:translationY="0dp"
               android:scaleX="1.5"
               android:scaleY="1.5" />
           <KeyAttribute
               motion:motionTarget="@+id/pictionary"
               motion:framePosition="85"
               android:alpha="1.0"
               android:translationY="0dp"
               android:scaleX="1.8"
               android:scaleY="1.8" />
           <KeyAttribute
               motion:motionTarget="@+id/pictionary"
               motion:framePosition="95"
               android:alpha="1.0"
               android:translationY="0dp"
               android:scaleX="3.0"
               android:scaleY="3.0" />
           <KeyAttribute
               motion:motionTarget="@+id/pictionary"
               motion:framePosition="100"
               android:alpha="1.0"
               android:translationY="0dp"
               android:scaleX="200"
               android:scaleY="200" />
           <KeyAttribute
               motion:motionTarget="@+id/white_screen"
               motion:framePosition="95"
               android:alpha="0.0" />
           <KeyAttribute
               motion:motionTarget="@+id/white_screen"
               motion:framePosition="100"
               android:alpha="1.0" />

       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="731dp"
            android:id="@+id/imageView" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/ludek"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:alpha="1.0"
            android:scaleY="1.0"
            android:scaleX="1.0"
            android:translationY="4dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="0dp" />
        <Constraint
            android:id="@+id/k"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:alpha="1.0"
            android:scaleY="1.0"
            android:scaleX="1.0"
            android:translationY="0dp"
            android:translationZ="1dp"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/blue_screen"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:translationX="0dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:rotation="0"
            android:translationY="0dp"
            android:translationZ="3dp"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/pictionary"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:alpha="1.0"
            android:translationY="0dp"
            android:translationZ="4dp"
            motion:layout_constraintStart_toStartOf="parent" />
    </ConstraintSet>
</MotionScene>